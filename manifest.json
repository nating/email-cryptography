{
  "manifest_version": 2,
  "name": "Hello World Extension",
  "version": "1.0",
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "content_scripts" : [
    {
      "matches": ["https://mail.google.com/*", "https://inbox.google.com/*"],
      "js": ["inboxsdk.js", "myapp.js","aes.js","api.js","cryptico.js","hash.js","jsbn.js","random.js","rsa.js"]
    }
  ],
  "browser_action": {
    "default_title": "Encrypt baby"
  }
}